<appDefinition>
   <appId>cicd_demo</appId>
   <id>cicd_demo</id>
   <version></version>
   <name>CI/CD Demo</name>
   <packageDefinitionList>
      <packageDefinition>
         <appId>cicd_demo</appId>
         <id>cicd_demo</id>
         <name>CI/CD Demo</name>
         <packageActivityFormMap>
            <entry>
               <string>customer_approver_process::approval</string>
               <packageActivityForm>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>approval</activityDefId>
                  <formId>customer_approval</formId>
                  <type>SINGLE</type>
                  <autoContinue>false</autoContinue>
                  <disableSaveAsDraft>false</disableSaveAsDraft>
               </packageActivityForm>
            </entry>
            <entry>
               <string>customer_approver_process::clarification</string>
               <packageActivityForm>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>clarification</activityDefId>
                  <formId>customer_clarification</formId>
                  <type>SINGLE</type>
                  <autoContinue>false</autoContinue>
                  <disableSaveAsDraft>false</disableSaveAsDraft>
               </packageActivityForm>
            </entry>
            <entry>
               <string>customer_approver_process::runProcess</string>
               <packageActivityForm>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>runProcess</activityDefId>
                  <formId>customer</formId>
                  <type>SINGLE</type>
                  <autoContinue>false</autoContinue>
                  <disableSaveAsDraft>false</disableSaveAsDraft>
               </packageActivityForm>
            </entry>
         </packageActivityFormMap>
         <packageActivityPluginMap>
            <entry>
               <string>customer_approver_process::email_on_approved</string>
               <packageActivityPlugin>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>email_on_approved</activityDefId>
                  <pluginName>org.joget.apps.app.lib.EmailTool</pluginName>
                  <pluginProperties>{     &quot;host&quot;: &quot;&quot;,     &quot;port&quot;: &quot;&quot;,     &quot;security&quot;: &quot;&quot;,     &quot;username&quot;: &quot;&quot;,     &quot;password&quot;: &quot;&quot;,     &quot;from&quot;: &quot;&quot;,     &quot;toSpecific&quot;: &quot;&quot;,     &quot;toParticipantId&quot;: &quot;customer_applicant&quot;,     &quot;cc&quot;: &quot;&quot;,     &quot;subject&quot;: &quot;Your Application Has Been Approved&quot;,     &quot;message&quot;: &quot;Hi #performer.runProcess.firstName# #performer.runProcess.lastName#,\n\nYour application has been approved on 30\/09\/2022 07:00 AM.\n\n==  This is a system generated message. == &quot;,     &quot;isHtml&quot;: &quot;true&quot;,     &quot;formDefId&quot;: &quot;&quot;,     &quot;fields&quot;: [],     &quot;files&quot;: [] }</pluginProperties>
               </packageActivityPlugin>
            </entry>
            <entry>
               <string>customer_approver_process::email_on_rejected</string>
               <packageActivityPlugin>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>email_on_rejected</activityDefId>
                  <pluginName>org.joget.apps.app.lib.EmailTool</pluginName>
                  <pluginProperties>{     &quot;host&quot;: &quot;&quot;,     &quot;port&quot;: &quot;&quot;,     &quot;security&quot;: &quot;&quot;,     &quot;username&quot;: &quot;&quot;,     &quot;password&quot;: &quot;&quot;,     &quot;from&quot;: &quot;&quot;,     &quot;toSpecific&quot;: &quot;&quot;,     &quot;toParticipantId&quot;: &quot;customer_applicant&quot;,     &quot;cc&quot;: &quot;&quot;,     &quot;subject&quot;: &quot;Your Application Has Been Rejected&quot;,     &quot;message&quot;: &quot;Hi #performer.runProcess.firstName# #performer.runProcess.lastName#,\n\nYour application has been rejected on 30\/09\/2022 07:00 AM.\n\n==  This is a system generated message. == &quot;,     &quot;isHtml&quot;: &quot;true&quot;,     &quot;formDefId&quot;: &quot;&quot;,     &quot;fields&quot;: [],     &quot;files&quot;: [] }</pluginProperties>
               </packageActivityPlugin>
            </entry>
            <entry>
               <string>customer_approver_process::update_status</string>
               <packageActivityPlugin>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>update_status</activityDefId>
                  <pluginName>org.joget.plugin.enterprise.FormDataUpdateTool</pluginName>
                  <pluginProperties>{&quot;formDefId&quot;:&quot;customer_approval&quot;,&quot;fields&quot;:[{&quot;field&quot;:&quot;approval_status&quot;,&quot;value&quot;:&quot;#variable.status#&quot;}]}</pluginProperties>
               </packageActivityPlugin>
            </entry>
            <entry>
               <string>customer_approver_process::email_on_received</string>
               <packageActivityPlugin>
                  <processDefId>customer_approver_process</processDefId>
                  <activityDefId>email_on_received</activityDefId>
                  <pluginName>org.joget.apps.app.lib.EmailTool</pluginName>
                  <pluginProperties>{     &quot;host&quot;: &quot;&quot;,     &quot;port&quot;: &quot;&quot;,     &quot;security&quot;: &quot;&quot;,     &quot;username&quot;: &quot;&quot;,     &quot;password&quot;: &quot;&quot;,     &quot;from&quot;: &quot;&quot;,     &quot;toSpecific&quot;: &quot;&quot;,     &quot;toParticipantId&quot;: &quot;customer_applicant&quot;,     &quot;cc&quot;: &quot;&quot;,     &quot;subject&quot;: &quot;Your Application Has Been Received&quot;,     &quot;message&quot;: &quot;Hi #performer.runProcess.firstName# #performer.runProcess.lastName#,\n\nYour application has been received on 30\/09\/2022 07:00 AM.\n\n==  This is a system generated message. == &quot;,     &quot;isHtml&quot;: &quot;true&quot;,     &quot;formDefId&quot;: &quot;&quot;,     &quot;fields&quot;: [],     &quot;files&quot;: [] }</pluginProperties>
               </packageActivityPlugin>
            </entry>
         </packageActivityPluginMap>
         <packageParticipantMap>
            <entry>
               <string>customer_approver_process::customer_applicant</string>
               <packageParticipant>
                  <processDefId>customer_approver_process</processDefId>
                  <participantId>customer_applicant</participantId>
                  <type>requester</type>
                  <value>runProcess</value>
               </packageParticipant>
            </entry>
            <entry>
               <string>customer_approver_process::customer_approver</string>
               <packageParticipant>
                  <processDefId>customer_approver_process</processDefId>
                  <participantId>customer_approver</participantId>
                  <type>requesterHod</type>
                  <value>runProcess</value>
               </packageParticipant>
            </entry>
            <entry>
               <string>customer_approver_process::processStartWhiteList</string>
               <packageParticipant>
                  <processDefId>customer_approver_process</processDefId>
                  <participantId>processStartWhiteList</participantId>
                  <type>role</type>
                  <value>adminUser</value>
               </packageParticipant>
            </entry>
         </packageParticipantMap>
      </packageDefinition>
   </packageDefinitionList>
   <formDefinitionList/>
   <userviewDefinitionList/>
   <datalistDefinitionList/>
   <builderDefinitionList/>
   <pluginDefaultPropertiesList/>
   <environmentVariableList/>
   <messageList/>
   <resourceList>
      <appResource>
         <id>team.png</id>
         <appId>cicd_demo</appId>
         <appVersion>1</appVersion>
         <filesize>54735</filesize>
         <permissionProperties>{&quot;hashvariable&quot;:&quot;#appResource.team.png#&quot;,&quot;permission&quot;:{&quot;className&quot;:&quot;&quot;,&quot;properties&quot;:{}}}</permissionProperties>
      </appResource>
      <appResource>
         <id>template.json</id>
         <appId>cicd_demo</appId>
         <appVersion>1</appVersion>
         <filesize>209</filesize>
         <permissionClass>org.joget.apps.userview.lib.LoggedInUserPermission</permissionClass>
         <permissionProperties>{&quot;permission&quot;: { &quot;className&quot;: &quot;org.joget.apps.userview.lib.LoggedInUserPermission&quot;, &quot;properties&quot;: {}}}</permissionProperties>
      </appResource>
   </resourceList>
